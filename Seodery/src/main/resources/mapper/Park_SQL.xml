<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="park">

	<!-- 댓글목록 조회 -->
 	<select id="getPDList" parameterType="hashmap" resultType="hashmap">
		select 
		    PC_SEQ.nextval, MBER_NUM, park_num, SRATING, CON
		FROM PC
		WHERE PARK_NUM = #{PARK_NUM}
	</select> 

	<!-- 댓글 목록 총 건수조회 -->
	 <select id="getPDCnt" parameterType="hashmap" resultType="Integer">
		select count(*) as cnt
		from PC
		WHERE PARK_NUM = #{PARK_NUM}
	</select> 


	<!-- <insert id="addO1" parameterType="hashmap">
		 INSERT INTO O1(O_NO,M_NO, CON)
		VALUES (O1_SEQ.NEXTVAL,#{mo},#{con})
	</insert> -->
</mapper>